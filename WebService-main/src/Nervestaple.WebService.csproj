<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <PackageId>Nervestaple.WebService</PackageId>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Version>0.4.4-rc</Version>
    <Company>Nervestaple Development</Company>
    <Product>Nervestaple.WebService</Product>
    <Description>Wraps Nervestaple.EntityFrameworkCore to help create database backed web services</Description>
    <RepositoryUrl>https://github.com/cmiles74/WebService.git</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.JsonPatch" Version="5.0.3" />
    <PackageReference Include="Nervestaple.EntityFrameworkCore" Version="0.5.1" />
  </ItemGroup>
  
  <PropertyGroup>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>
</Project>
