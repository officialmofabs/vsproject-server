            "scope": "variable",
            "settings": {
                "foreground": "#d9b974",
            }
        }, */
        {
          "scope": "comment",
          "settings": {
              "foreground": "#676767",
              "fontStyle": "italic"
          },
        },
        {
          "scope": "keyword",
          "settings": {
              "foreground": "#9e00c5",
          },
        },
        {
