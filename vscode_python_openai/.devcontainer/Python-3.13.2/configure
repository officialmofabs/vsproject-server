  iOS) :

    as_fn_append CFLAGS " -mios-version-min=${IPHONEOS_DEPLOYMENT_TARGET}"
    as_fn_append LDFLAGS " -mios-version-min=${IPHONEOS_DEPLOYMENT_TARGET}"
   ;; #(
  *) :
     ;;
esac

if test "$ac_sys_system" = "Darwin"
