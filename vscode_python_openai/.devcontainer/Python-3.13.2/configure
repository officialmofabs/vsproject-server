if test "$GCC" = yes; then
       ac_prog=`$CC -print-prog-name=ld`
fi
case `"$ac_prog" -V 2>&1 < /dev/null` in
      *GNU*)
          GNULD=yes;;
      *)
          GNULD=no;;
esac
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $GNULD" >&5
printf "%s\n" "$GNULD" >&6; }

